{
  "extends": "../tsconfig.json",
  "exclude": [],
  "include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.mjs",
    "**/*.cjs",
    "**/*.ts",
    "**/*.tsx",
    "../src/**/*.ts",
    "../src/**/*.tsx",
    "next.config.mjs",
    ".next/types/**/*.ts"
, "../jest.config.js"  ],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@payload-config": [
        "./payload.config.ts"
      ],
      "@tmrrw-labs/payload-plugin-label-popover": [
        "../src/index.ts"
      ],
      "@tmrrw-labs/payload-plugin-label-popover/client": [
        "../src/exports/client.tsx"
      ],
      "@tmrrw-labs/payload-plugin-label-popover/rsc": [
        "../src/exports/rsc.tsx"
      ]
    },
    "noEmit": true,
    "emitDeclarationOnly": false,
  }
}
